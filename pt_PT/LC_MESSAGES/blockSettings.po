msgid "LOGIN_REQUIRED_TO_MANAGE_BLOCKS"
msgstr "Precisas de entrar na tua conta para gerir utilizadores bloqueados."

msgid "NO_USERS_BLOCKED"
msgstr "Não tens utilizadores bloqueados."

msgid "BLOCKED_USERS"
msgstr "Utilizadores Bloqueados"

msgid "BLOCK_USER"
msgstr "Bloquear utilizador"

msgid "COMMENT_BLOCK_IS_MUTUAL"
msgstr "Depois de bloqueares este utilizador, não poderás comentar nos Flipnotes de um do outro."

msgid "USER_HAS_BEEN_BLOCKED"
msgstr "Este utilizador foi bloqueado."

msgid "IF_YOU_WISH_TO_UNBLOCK"
msgstr "Se desejares desbloquear o utilizador, podes desbloquear-lo por ir na Sala de Criador e abrir o menu de Utilizadores Bloqueados."

msgid "CONFIRM_BLOCK"
msgstr "Confirm Block"

msgid "USER_ALREADY_BLOCKED"
msgstr "Tu já bloqueaste este utilizador. Podes desbloquear-lo no menu Definições na Sala de Criador, mas tens que esperar 24 horas para o bloquear de novo."

msgid "ONE_DAY_WAIT_BEFORE_REBLOCK_FORMAT"
msgstr "Bloqueaste recentemente este utilizador. Não o podes bloquear até %s"

msgid "UNBLOCK"
msgstr "Desbloquear"

msgid "LOGIN_REQUIRED"
msgstr "Tens que entrar na tua conta para executar esta ação."

msgid "TAP_BACK_TO_RETURN"
msgstr "Pressiona Voltar para voltar a Sala de Criador."

msgid "UNBLOCK_USER"
msgstr "Desbloquear utilizador"

msgid "CANNOT_UNBLOCK_NONBLOCKED_USER"
msgstr "Não podes desbloquear este utilizador, porque não o bloqueaste em primeiro lugar."

msgid "USER_HAS_BEEN_UNBLOCKED"
msgstr "Desbloqueaste este utilizador."

msgid "COMMENTING_ENABLED_AFTER_UNBLOCK"
msgstr "Depois de desbloqueares este utilizador, ele pode comentar nos teus Flipnotes de novo." 

msgid "UNBLOCK_WARNING_ONE_DAY_THRESHOLD"
msgstr "Se desbloqueares um utilizador, tens que esperar 24 horas para o bloquear de novo."

msgid "CONFIRM_UNBLOCK"
msgstr "Confirmar desbloqueamento"

msgid "RECENTLY_UNBLOCKED_ONE_DAY_THRESHOLD"
msgstr "Desbloqueaste este utilizador recentemente. Tens que esperar 24 horas para o bloquear de novo."
